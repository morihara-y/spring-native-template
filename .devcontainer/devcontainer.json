{
  // change the name for your project
  "name": "spring-native-template",
  "dockerComposeFile": "docker-componse.yml",
  "service": "worksapce",
  // change the name for your project
  "workspaceFolder": "/spring-native-template",
  "extensions": [
    "vscjava.vscode-java-pack",
    "gabrielbb.vscode-lombok"
